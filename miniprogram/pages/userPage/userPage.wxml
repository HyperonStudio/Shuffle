<!--miniprogram/pages/UserPage/UserPage.wxml-->
<!-- 如果只是展示用户头像昵称，可以使用 <open-data /> 组件 -->
<view class="userinfo">
  <view class="userinfo-avatar">
    <open-data  type="userAvatarUrl"></open-data>
  </view>
  <view class='infoarea'>
    <open-data type="userNickName"></open-data>
    <view class='collect'>
    <view class='collect-container'>
    <label class='collect-label'>被收藏</label>
    <label class='collect-num'>0</label>
    </view>
    <view class='collect-container'>
    <label class='collect-label'>收藏</label>
    <label class='collect-num'>0</label>
     </view>
    </view>
  </view>
</view>
<view class='collections'>
<view class='switch-container'>
<label class='switchLabel'>发布</label>
<label class='switchLabel'>收藏</label>
</view>

<view class='card-container'>
<image class='card' src='../../images/loocup_cover_0.jpeg'> 

</image>
<image class='card' src='../../images/loocup_cover_1.png'> 
<view class='stars'>
<image class='stars-logo'></image>
<label class='stars-num'>333</label>
</view>
</image>
<image class='card' src='../../images/loocup_cover_2.jpeg'> </image>
<image class='card' src='../../images/loocup_cover_3.jpeg'> </image>
<image class='card' src='../../images/loocup_cover_4.jpeg'> </image>
<image class='card' src='../../images/loocup_cover_5.jpeg'> </image>

</view>

</view>  
